{
  "name": "firefly-api",
  "version": "1.0.0",
  "description": "OpenAPI Autogenerated firefly-iii TypeScript API",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "generate": "openapi-generator-cli generate -i firefly-iii-spec.yaml -g typescript-axios -o . -c ./openapi-generator.json",
    "fetch": "curl -o firefly-iii-spec.yaml https://api-docs.firefly-iii.org/firefly-iii-2.0.0-v1.yaml && npm run replace",
    "replace": "replace-in-file --configFile=replace.js",
    "ci": "npm run fetch && npm run generate && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/6ixfalls/firefly-api.git"
  },
  "keywords": [
    "OpenAPI",
    "OpenAPI-Generator",
    "firefly-iii",
    "typescript",
    "axios"
  ],
  "bugs": {
    "url": "https://github.com/6ixfalls/firefly-api/issues"
  },
  "homepage": "https://github.com/6ixfalls/firefly-api#readme",
  "author": "6ixfalls",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.6.0",
    "@types/node": "^18.16.2",
    "replace-in-file": "^6.3.5",
    "typescript": "^5.0.4"
  }
}
